{
  "name": "git-tidier",
  "version": "1.2.0",
  "description": "cli tool to clean git branch interactively",
  "bin": {
    "git-tidier": "./bin/git-tidier.js"
  },
  "scripts": {
    "format": "npx prettier --write \"{src,tests}/**/*.{js,vue,ts,tsx}\"",
    "format:check": "npx prettier --check \"{src,tests}/**/*.{js,vue,ts,tsx}\"",
    "lint": "npx eslint \"{src,tests}/**/*.{js,vue,ts,tsx}\" --max-warnings 0",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests}/**/*.{js,vue,ts,tsx}\" --fix",
    "test": "npx jest --ci",
    "jest:coverage": "npx jest --ci --coverage",
    "jest:collectCoverage": "npx jest --collectCoverage"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.11",
    "@inquirer/prompts": "^4.1.0",
    "@types/jest": "^29.5.3",
    "chalk": "^4.1.0",
    "commander": "^12.1.0",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.21",
    "log4js": "^6.4.0",
    "minimatch": "^9.0.3",
    "moment": "^2.29.4",
    "shelljs": "^0.8.5",
    "string-format": "^2.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^29.6.1",
    "babel-preset-env": "^1.7.0",
    "regenerator-runtime": "^0.14.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "eslint": "^9.8.0",
    "eslint-plugin-jest": "^28.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "semver": "^7.6.0"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "keywords": [
    "clear",
    "git",
    "git branch",
    "node",
    "cli",
    "node-cli"
  ],
  "author": "sinlovgmppt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bridgewwater/git-tidier.git"
  },
  "homepage": "https://github.com/bridgewwater/git-tidier#readme",
  "bugs": {
    "url": "https://github.com/bridgewwater/git-tidier/issues"
  }
}